{
  "expo": {
    "name": "CustomerApplication",
    "slug": "CustomerApplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashlogo.png",
    "scheme": "myapp",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splashlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDAA48"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["myapp"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["myapp"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashlogo.png",
        "backgroundColor": "#FDAA48"
      },
      "intentFilters": [
        {
          "action": { "VIEW": "android.intent.action.VIEW" },
          "data": [
            {
              "scheme": "myapp",
              "host": "payment"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
